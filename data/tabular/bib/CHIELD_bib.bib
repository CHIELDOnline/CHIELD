	Title = {Basic Words and Language Evolution},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/wang04basicWords.html},
	Volume = {5},
	Year = {2004},
	Bdsk-Url-1 = {http://www.isrl.uiuc.edu/~amag/langev/paper/wang04basicWords.html}}

