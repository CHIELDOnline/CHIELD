	Keywords = {language acquisition, population dynamics, creole},
	Publisher = {CSREA Press},
	Title = {The Language Dynamics Equations of Population-Based Transition -- a Scenario for Creolization},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/nakamura03icai.html},
