@article{cangelosi_parisi_BrainLanguage,
	Abstract = {The paper presents a computational model of language in which linguistic abilities evolve in organisms that interact with an environment. Each individualÂ¿s behavior is controlled by a neural network and we study the consequences in the networkÂ¿s internal functional organization of learning to process different classes of words. Agents are selected for reproduction according to their ability to manipulate objects and to understand nouns (objectsÂ¿ names) and verbs (manipulation tasks). The weights of the agentsÂ¿ neural networks are evolved using a genetic algorithm. Synthetic brain imaging techniques are then used to examine the functional organization of the neural networks. Results show that nouns produce more integrated neural activity in the sensory-processing hidden layer, while verbs produce more integrated synaptic activity in the layer where sensory information is integrated with proprioceptive input. Such findings are qualitatively compared with human brain imaging data that indicate that nouns activate more the posterior areas of the brain related to sensory and associative processing, while verbs activate more the anterior motor areas.},
	Author = {A. Cangelosi and D. Parisi},
	Date-Added = {2017-12-18 14:49:46 +0000},
	Date-Modified = {2017-12-18 14:49:46 +0000},
	Doi = {10.1016/S0093-934X(03)00353-5},
	Journal = {Brain and Language},
	Number = {2},
	Pages = {401-408},
	Title = {The processing of verbs and nouns in neural networks: Insights from Synthetic Brain Imaging},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/cangelosi_parisi_BrainLanguage.html},
	Volume = {89},
	Year = {2004},
	Bdsk-Url-1 = {http://www.isrl.uiuc.edu/~amag/langev/paper/cangelosi_parisi_BrainLanguage.html},
