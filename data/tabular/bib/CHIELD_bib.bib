@unpublished{nakhleh05JLSA,
	Abstract = {In this paper we extend the Ringe-Warnow model of language evolution to include the case where languages remain in contact, trading linguistic material, as they evolve. We describe our analysis of an Indo-European dataset (originally assembled by Ringe and Taylor) based on this new model. Our study shows that this new model fits the IE family well and suggests that the early evolution of IE involved only limited contact between distinct lineages. Furthermore, the candidate histories we obtain appear to be consistent with archaeological findings, which suggests that this method may be of practical use.},
	Author = {L. Nakhleh and D. Ringe and T. Warnow},
