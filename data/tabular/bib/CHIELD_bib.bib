	Doi = {10.1016/S0010-0277(96)00728-7},
	Journal = {Cognition},
	Month = {Oct-Nov},
	Number = {1-2},
	Pages = {1-38},
	Title = {A computational study of cross-situational techniques for learning word-to-meaning mappings},
