	Bdsk-Url-2 = {http://dx.doi.org/10.1109/TEVC.2007.892763}}

@article{fontanari04wordFrequencyPRE,
	Abstract = {Zipf's law asserts that in all natural languages the frequency of a word is inversely proportional to its rank. The significance, if any, of this result for language remains a mystery. Here we examine a null hypothesis for the distribution of word frequencies, a so-called discourse-triggered word choice model, which is based on the assumption that the more a word is used, the more likely it is to be used again. We argue that this model is equivalent to the neutral infinite-alleles model of population genetics and so the degeneracy of the different words composing a sample of text is given by the celebrated Ewens sampling formula [<article>Theor. Pop. Biol. 3, 87 (1972)</article>], which we show to produce an exponential distribution of word frequencies.},
	Author = {J.F. Fontanari and L.I. Perlovsky},
	Date-Added = {2017-12-18 14:49:46 +0000},
	Date-Modified = {2017-12-18 14:49:46 +0000},
	Journal = {Physical Review E},
	Month = {Oct},
	Number = {4},
	Pages = {042901},
