	Doi = {10.1007/s10339-006-0158-3},
	Journal = {Cognitive Processing},
	Keywords = {language evolution, language change, Darwinian evolution, genes, memes, replicators,learning bias, functional pressure, self-organization},
	Month = {January},
	Number = {1},
