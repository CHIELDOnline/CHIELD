	Journal = {Neural Networks},
	Keywords = {Brain evolution; Broca's area;Cortical maps; Homologies; Neural; Language; Neural mechanisms; Mirror neurons; NeuroHomology Database; Neuroinformatics; Neurolinguistics; Wernicke's area},
	Number = {9},
	Pages = {1237-1260},
	Title = {Language evolution: neural homologies and neuroinformatics},
