@article{kalampokis07vocabularyOnNetworks,
	Abstract = {We examine the evolution of the vocabulary of a group of individuals (linguistic agents) on a scale-free network, using Monte Carlo simulations and assumptions from evolutionary game theory. It is known that when the agents are arranged in a two-dimensional lattice structure and interact by diffusion and encounter, then their final vocabulary size is the maximum possible. Knowing all available words is essential in order to increase the probability to 'survive' by effective reproduction. On scale-free networks we find a different result. It is not necessary to learn the entire vocabulary available. Survival chances are increased by using the vocabulary of the 'hubs' (nodes with high degree). The existence of the 'hubs' in a scale-free network is the source of an additional important fitness generating mechanism. (C) 2007 Elsevier B.V. All rights reserved.},
	Author = {Alkiviadis Kalampokis and Kosmas Kosmidis and Panos Argyrakis},
	Date-Added = {2017-12-18 14:49:46 +0000},
	Date-Modified = {2017-12-18 14:49:46 +0000},
	Doi = {10.1016/j.physa.2006.12.048},
	Journal = {Physica A: Statistical Mechanics and its Applications},
	Keywords = {language evolution; scale-free networks; Monte Carlo simulations},
	Month = {Jun},
	Number = {2},
	Pages = {665-671},
	Title = {Evolution of vocabulary on scale-free and random networks},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/kalampokis07vocabularyOnNetworks.html},
	Volume = {379},
	Year = {2007},
