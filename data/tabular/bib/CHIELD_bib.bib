	Editor = {G. N. Gilbert and R. Conte},
	Publisher = {UCL Press},
	Title = {How to invent a lexicon: the development of shared symbols in interaction},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/hutchins95howTo.html},
	Year = {1995},
	Bdsk-Url-1 = {http://www.isrl.uiuc.edu/~amag/langev/paper/hutchins95howTo.html}}

@inproceedings{hutchins92alife,
	Abstract = {In this paper, we introduce a framework for simulating cultural process. The general idea is to simulate a world in which learning agents create external structures which mediate their behavior. This simulation demonstrates the simple (but very important) concept that such a system is capable, through generations of time, of producing agents endowed with cognitive powers that are not attainable in the lifetime of any individual agent. We argue that such an outcome is possible without effecting the genetic organization of individuals. However, we also argue that the cultural process is capable of guiding both learning and phylogenetic evolution, which leaves open the possiblity of genetic organization tracking cultural process.},
	Author = {Edwin Hutchins and Brian Hazlehurst},
	Booktitle = {Artificial Life II},
	Date-Added = {2017-12-18 14:49:46 +0000},
