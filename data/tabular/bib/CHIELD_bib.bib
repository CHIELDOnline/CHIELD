	Doi = {10.1177/105971230501300402},
	Journal = {Adaptive Behavior},
	Keywords = {language acquisition,language change,language evolution,meaning inference,cultural transmission,cross-situational learning},
	Number = {4},
	Pages = {311-324},
	Title = {The Inferential Transmission of Language},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/admSmith05inferentialTransmissionABJ.html},
	Volume = {13},
	Year = {2005},
