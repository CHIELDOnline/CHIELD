	Keywords = {Compositionality; Grammar induction; Grounding; Iterated learning; Language evolution; Language games},
	Month = {September},
	Number = {1-2},
	Pages = {206-242},
	Title = {The emergence of compositional structures in perceptually grounded language games},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/vogt05compositionalGrounded_AIJ.html},
