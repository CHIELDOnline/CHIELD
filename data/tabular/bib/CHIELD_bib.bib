@incollection{hutchins95howTo,
	Abstract = {In this paper, we elaborate upon the framework by considering more explicitly the problem of creating shared symbolic structure. A lexicon is (among other things) a set of public structures for denoting or implicating shared meanings. In other words, the existence of a lexicon requires the sharing of forms and meanings -- and mappings between these -- among members of an interacting population of agents. Leaving aside cosmic and theological events which could create such an outcome, how could a lexicon come to be? The solution provided here is based upon a convergence of agents' schemes for classifying visual phenomena. All agents have a capacity for such classification, but convergence upon a singular scheme is shaped by the constraints for consensus when employing the scheme in interaction with other agents.},
	Address = {London},
	Author = {E. Hutchins and B. Hazlehurst},
	Booktitle = {Artificial Societies: The computer simulation of social life},
	Date-Added = {2017-12-18 14:49:46 +0000},
