	Journal = {Journal of Theoretical Biology},
	Number = {1},
	Pages = {71-76},
	Title = {Is There Any Intermediate Stage Between Animal Communication and Language?},
	Url = {http://www.isrl.uiuc.edu/~amag/langev/paper/ujhelyi96jtb.html},
	Volume = {180},
	Year = {1996},
